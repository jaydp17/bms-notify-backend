{
  "name": "bms-notify-backend",
  "version": "0.0.1",
  "author": "Jaydeep Solanki <jaydp17@gmail.com> (https://jaydp.com)",
  "bugs": {
    "url": "https://github.com/jaydp17/bms-notify-backend/issues"
  },
  "dependencies": {
    "aws-sdk": "^2.418.0",
    "axios": "^0.18.0",
    "lodash": "^4.17.11"
  },
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-typescript": "^7.3.3",
    "@types/lodash": "^4.14.122",
    "@typescript-eslint/eslint-plugin": "^1.4.2",
    "@typescript-eslint/parser": "^1.4.2",
    "babel-loader": "^8.0.5",
    "babel-plugin-lodash": "^3.3.4",
    "eslint": "^5.15.1",
    "eslint-config-prettier": "^4.1.0",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.5",
    "npm-run-all": "^4.1.5",
    "prettier": "1.16.4",
    "serverless": "^1.38.0",
    "serverless-appsync-offline": "^1.1.2",
    "serverless-appsync-plugin": "^1.1.0",
    "serverless-offline": "^4.8.1",
    "serverless-pseudo-parameters": "^2.4.0",
    "serverless-webpack": "^5.2.0",
    "terser-webpack-plugin": "^1.2.3",
    "ts-node": "^8.0.3",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "typescript": "3.3.3333",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.2.3"
  },
  "homepage": "https://github.com/jaydp17/bms-notify-backend#readme",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "run-p lint type-check"
    }
  },
  "keywords": [],
  "license": "MIT",
  "lint-staged": {
    "*.{ts,js}": [
      "eslint --config .eslintrc.js --fix",
      "prettier --write",
      "git add"
    ],
    "*.{json,yaml,yml,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "main": "index.js",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jaydp17/bms-notify-backend.git"
  },
  "resolutions": {
    "nan": "^2.12.1",
    "node-gyp": "^3.8.0"
  },
  "scripts": {
    "debug": "NODE_DEBUG=appsync-emulator:* node --inspect ./node_modules/.bin/sls offline start",
    "dev": "sls offline start",
    "eslint-check": "eslint --print-config . | eslint-config-prettier-check",
    "format": "prettier --write '**/*.{js,ts,json,yaml,yml,md}'",
    "invoke:local": "sls invoke local --log -f",
    "lint": "eslint --ext '.ts,.js'--fix .",
    "ts-run": "ts-node --compiler-options '{\"module\": \"commonjs\", \"target\":\"es2015\"}'",
    "type-check": "tsc"
  }
}
